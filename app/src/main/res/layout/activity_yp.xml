<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.YpActivity">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/list_back_t"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageButton
                    android:id="@+id/btn_back"
                    android:layout_width="40dp"
                    android:layout_height="30dp"
                    android:background="#1074fd"
                    android:src="@mipmap/back"
                    android:layout_marginTop="5dp">
                </ImageButton>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="8">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:paddingLeft="146dp"
                    android:text="押瓶"
                    android:textColor="#ffffff"
                    android:textSize="20sp"
                    android:layout_marginTop="5dp">

                </TextView>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="客户"
                android:textColorHint="#000000"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/kehu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint=""
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:screenReaderFocusable="false"
                android:textColorHint="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="气瓶规格"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <Spinner
                android:id="@+id/qipingguige"
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:entries="@array/peisongleixing"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="押金"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/yajin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="请输入押金"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="开户费"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/kaihufei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="请输入开户费"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="折价费"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/zhejiafei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="请输入折价费"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="应补差价"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/yingbuchajia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:text="0"
                android:focusable="false"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="押瓶数量"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <EditText
                android:id="@+id/yapingshuliang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:text="1"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:layout_gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="押瓶日期"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"
                android:textSize="15sp"
                android:layout_marginTop="3dp"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/yapingriqi"
                android:layout_width="190dp"
                android:layout_height="40dp"
                android:textColor="@color/black"
                android:hint="请点击选择押瓶日期"
                android:textAlignment="viewEnd"
                android:textSize="15sp"
                android:layout_marginTop="3dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape_list_list"
            android:orientation="horizontal"
            android:layout_gravity="center">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:hint="到期日期"
                android:textColor="#000000"
                android:textColorLink="#000000"
                android:textColorHint="#000000"
                android:focusable="false"
                android:textSize="15sp"
                android:layout_marginTop="3dp"/>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/daoqiriqi"
                android:layout_width="190dp"
                android:layout_height="40dp"
                android:textColor="@color/black"
                android:hint="请点击选择到期日期"
                android:textAlignment="viewEnd"
                android:textSize="15sp"
                android:layout_marginTop="3dp"/>

        </LinearLayout>





        <Button
            android:id="@+id/btn_submit"
            android:layout_width="294dp"
            android:layout_height="52dp"
            android:layout_gravity="center"
            android:layout_marginTop="19dp"
            android:background="@drawable/but_sub"
            android:text="提交">
        </Button>

        <Button
            android:id="@+id/btn_cancel"
            android:layout_width="294dp"
            android:layout_height="52dp"
            android:layout_gravity="center"
            android:layout_marginTop="19dp"
            android:background="#ffffff"
            android:text="取消">
        </Button>





    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>